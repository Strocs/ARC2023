{
  "name": "festival-arc-2023",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/tailwind": "^2.1.3",
    "astro": "^1.9.0",
    "astro-icon": "^0.8.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "astro-eslint-parser": "^0.9.5",
    "eslint": "^8.31.0",
    "eslint-plugin-astro": "^0.21.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:astro/recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021,
      "ecmaFeatures": {
        "globalReturn": false,
        "impliedStrict": false,
        "jsx": false
      }
    },
    "overrides": [
      {
        "files": [
          "*.astro"
        ],
        "parser": "astro-eslint-parser"
      }
    ]
  }
}
